# Functions call
PrintLine("Hello, world!");

# Declaring variables
hello := "Hello";
n := hello;
n = "Bob";
PrintLine(hello + ", " + n + "!");

# if
if(false) {
    PrintLine(1);
}
else {
    PrintLine(2);
}

# while
b := 5;
while(b > 0) {
    PrintLine(b);
    b = b - 1;
}

# for
for(c := 0, c <= 10, c = c + 1) {
    PrintLine(c * c);
}

# foreach
for(z in [1, 2, 3]) {
    PrintLine(z);
}

# foreach in range
for(z in [1..3]) {
    PrintLine(z);
}

#funcs
func PrintName(name) {
    PrintLine(name);
}

PrintName("Bob");

# build-in funcs
PrintLine(TypeOf("Bob"));

# arrays
e := [1, 2, 3];
PrintName(e);

# dicts
g := {"name": "Vlad", "age": 19};
PrintName(g);